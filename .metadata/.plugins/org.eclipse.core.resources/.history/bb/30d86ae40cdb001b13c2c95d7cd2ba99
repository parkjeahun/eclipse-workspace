package arraylist;
import java.util.Scanner;
public class Q12 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("양의 정수 10개를 입력하시오");
		
		for(int intager = sc.nextInt(10);) {
		 nana = intager % 3;
		continue;
		
		}
		System.out.println("3의배수는" + nana);
	}

}
