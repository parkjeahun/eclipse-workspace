package exam;
import java.util.Iterator;
import java.util.Scanner;
public class Q12 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[] = new int [10];
		
		System.out.println("양의 점수 10개를 입력 하시오 >> ");
		
		for (int i =0; i < arr.length; i++) {
			arr[i] = sc.nextInt();
			
		}
		for(int i=0; i < arr.length; i++);
	}

}
