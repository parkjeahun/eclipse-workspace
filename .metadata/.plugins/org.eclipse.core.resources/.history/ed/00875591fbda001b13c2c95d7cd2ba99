package arraylist;

public class Q3 {

	public static void main(String[] args) {
		int linecount = 4;
		int starcount = 1;
		
		for(int i = 0; i < linecount; i++) {
			System.out.print(' ');
		}
		for(int j = 0; j < starcount; j++) {
			System.out.print("*");
		}
		starcount+=1;
		System.out.println();
	}

}
