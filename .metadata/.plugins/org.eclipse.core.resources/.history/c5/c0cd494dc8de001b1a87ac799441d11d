package upcasting;

public class upcastingEx {

	public static void main(String[] args) {
		Person p;
		Student s = new Student("박재훈");
		
		p = s;			//업캐스팅 
		System.out.println(p.name);
		
		//p.grade = "A";		//컴파일 오류
		//p.departmernt = "HR"; //컴파일 오류
		
		s.grade = "A";
		s.department = "HR";
	}
}
