package arraylist;
public class Q14 {

	public static void main(String[] args) {
		int[] nana = new int[10];
		
		System.out.println("정수를 입력하세요");
		int num = 0;
		for(int i = 0; i<10; i++) {
			int i = (int)(Math.random())*10+1);
			num +=nana[i];
		System.out.println(nana[i] " ");
		}
		System.out.println();
		System.out.println("평균값은:" + num / 10 + "입니다.");
		
		
		
	}

}
