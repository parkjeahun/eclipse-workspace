package arraylist;

public class Max {

	public static void main(String[] args) {
        int max = 0;
        int[] array = {4,5,1,2,3};
		for(int i = 0; i < array.length; i++) {
		}
		System.out.println();
		System.out.println("방번호(인덱스):" + array[0]);
		System.out.println(array.length);
		
        System.out.println("max: " + max);
    }}

	


